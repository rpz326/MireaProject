<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/dataFragment">

    <fragment
        android:id="@+id/dataFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.DataFragment"
        android:label="fragment_data"
        tools:layout="@layout/fragment_data" />
    <fragment
        android:id="@+id/webViewFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.WebViewFragment"
        android:label="fragment_web_view"
        tools:layout="@layout/fragment_web_view" />
    <fragment
        android:id="@+id/factorialFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.FactorialFragment"
        android:label="fragment_factorial"
        tools:layout="@layout/fragment_factorial" />
    <fragment
        android:id="@+id/sensorFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.SensorFragment"
        android:label="fragment_sensor"
        tools:layout="@layout/fragment_sensor" />
    <fragment
        android:id="@+id/noteFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.NoteFragment"
        android:label="fragment_note"
        tools:layout="@layout/fragment_note" />
    <fragment
        android:id="@+id/volumeFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.VolumeFragment"
        android:label="fragment_volume"
        tools:layout="@layout/fragment_volume" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.ProfileFragment"
        android:label="fragment_profile"
        tools:layout="@layout/fragment_profile" />
    <fragment
        android:id="@+id/fileFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.FileFragment"
        android:label="fragment_file"
        tools:layout="@layout/fragment_file" />
    <fragment
        android:id="@+id/fileCreateFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.FileCreateFragment"
        android:label="fragment_file_create"
        tools:layout="@layout/fragment_file_create" />
    <fragment
        android:id="@+id/fileEditFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.FileEditFragment"
        android:label="fragment_file_edit"
        tools:layout="@layout/fragment_file_edit">
        <argument
            android:name="fileName"
            app:argType="string" />
    </fragment>
    <action
        android:id="@+id/action_fileFragment_to_fragmentFileEdit"
        app:destination="@id/fileEditFragment" />
    <fragment
        android:id="@+id/userFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.UserFragment"
        android:label="fragment_user"
        tools:layout="@layout/fragment_user" />
    <fragment
        android:id="@+id/exchangeRatesFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.ExchangeRatesFragment"
        android:label="fragment_exchange_rates"
        tools:layout="@layout/fragment_exchange_rates" />
    <fragment
        android:id="@+id/mapFragment"
        android:name="ru.mirea.sukhanovmd.mireaproject.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" />
</navigation>